{
  "name": "gardenbuilder-api-typescript",
  "version": "1.0.0",
  "description": "API for Gardenbuilder",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "database:seed": "psql postgres < ./seed/plant_options.sql",
    "deploy": "gcloud app deploy",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc '**/*.ts' --write",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "npm run test:clear-database && jest --coverage",
    "test:jest": "NODE_ENV=test jest",
    "test:clear-database": "NODE_ENV=test ts-node ./tests/testUtils/clearTestDatabase.ts",
    "trefle": "NODE_ENV=development ts-node-dev src/utils/queryTrefle.ts",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gardenbuilder-app/gardenbuilder-backend-typescript.git"
  },
  "keywords": [
    "typescript",
    "gardening",
    "node",
    "graphql",
    "typeorm"
  ],
  "author": "David Thompson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gardenbuilder-app/gardenbuilder-backend-typescript/issues"
  },
  "homepage": "https://github.com/gardenbuilder-app/gardenbuilder-backend-typescript#readme",
  "dependencies": {
    "@google-cloud/debug-agent": "^5.1.3",
    "@google-cloud/storage": "^5.7.0",
    "apollo-server": "^2.19.0",
    "argon2": "^0.27.1",
    "axios": "^0.21.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.14",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.0",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "pre-commit": [
    "format",
    "lint",
    "test"
  ]
}
